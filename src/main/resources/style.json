{
  "atxheader1": {
  "level": 1,
  "fontSize": 96,
  "bold" : false,
    "regex": "^#{1}[\\s]+.*"
},
  "atxheader2": {
    "level": 2,
    "fontSize": 60,
    "bold" : false,
    "regex": "^#{2}[\\s]+.*"
  },
  "atxheader3": {
    "level" : 3,
    "fontSize": 48,
    "bold" : false,
    "regex": "^#{3}[\\s]+.*"
  },
  "atxheader4": {
    "level" : 4,
    "fontSize": 34,
    "bold" : false,
    "regex": "^#{4}[\\s]+.*"
  },
  "atxheader5": {
    "level" : 5,
    "fontSize": 24,
    "bold" : false,
    "regex": "^#{5}[\\s]+.*"
  },
  "atxheader6": {
    "level" : 6,
    "fontSize": 20,
    "bold" : false,
    "regex": "^#{6}[\\s]+.*"
  },
  "setextheader1": {
    "level": 1,
    "fontSize": 96,
    "bold" : false,
    "regex": "^[\\s]{0,3}[\\S]+.*((\\r\\n|\\r|\\n))=+"
  },
  "setextheader2": {
    "level": 2,
    "fontSize": 60,
    "bold" : false,
    "regex": "^[\\s]{0,3}[\\S]+.*((\\r\\n|\\r|\\n))-+"

  },
  "paragraph": {
    "fontSize": 16,
    "regex": "^[^\\n\\r#].*"
  },

  "indentedcode": {
    "fontSize": 16,
    "bold": false,
    "font": "Monospaced",
    "regex": "^([\\s]{4,}|[\\t]+)"
  }

}